package application;

import java.util.Locale;
import java.util.Scanner;

public class Matrix {

	public static void main(String[] args) {

		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int x = sc.nextInt();
		int[][] mat = new int[x][x];
		
		for (int m=0; m<mat.length; m++) {
			for (int n=0; n<mat[m].length; n++) {
				mat[m][n] = sc.nextInt();
			}
		}
		
		sc.close();

	}

}

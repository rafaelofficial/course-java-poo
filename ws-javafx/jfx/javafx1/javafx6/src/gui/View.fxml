<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ViewController">
   <children>
      <ComboBox fx:id="comboBoxPerson" layoutX="50.0" layoutY="62.0" onAction="#onComboBoxPersonAction" prefWidth="150.0" promptText="Select a person..." />
      <Button fx:id="btnAll" layoutX="250.0" layoutY="62.0" mnemonicParsing="false" onAction="#onBtnAllAction" prefHeight="25.0" prefWidth="59.0" text="All" />
   </children>
</AnchorPane>
